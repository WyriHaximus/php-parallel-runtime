{
  "name": "wyrihaximus/parallel-runtime",
  "description": "\ud83c\udf81 ext-parallel runtime wrapping adding error, signal, and exception handling",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-parallel": "^1",
    "wyrihaximus/constants": "^1.6"
  },
  "require-dev": {
    "thecodingmachine/safe": "^1.3",
    "wyrihaximus/async-test-utilities": "^3.3"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "php",
      "ext-parallel"
    ]
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\Parallel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\Parallel\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
